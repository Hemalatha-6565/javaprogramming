import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner hema=new Scanner(System.in);
        int a=hema.nextInt();
        int b=hema.nextInt();
        int c=hema.nextInt();
        int d=hema.nextInt();
        System.out.println("Items Details");
        System.out.println("A4sheet:"+ a);
        System.out.println("Pen:"+ b);
        System.out.println("Pencil:"+ c);
        System.out.println("Eraser:"+ d);
        
    }
        
}
