import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int po=0;
        int ne=0;
    for (int i=0;i<n;i++) {
        int num=scan.nextInt();
           if (num>0) {
                po++;
            } else if(num<0){
                ne++;
            }
        }

        System.out.printf("Count of Positive Integer is %.2f\n", (float) po);
        System.out.printf("Count of Negative Integer is %.2f\n", (float) ne);
    }
}
