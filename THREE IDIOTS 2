import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
     Scanner scan=new Scanner(System.in);
        int x1=scan.nextInt();
        int y1=scan.nextInt();
        int x2=scan.nextInt();
        int y2=scan.nextInt();
        double a=(x1+x2)/2.0;
        double b=(y1+y2)/2.0;
        System.out.printf("Binoy's house is located at "+"("+a+","+b+")");
    
        
    }
}
