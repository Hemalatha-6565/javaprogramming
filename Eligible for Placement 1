import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner hema=new Scanner(System.in);
        String str=hema.nextLine();
        int ar=hema.nextInt();
        int cg=hema.nextInt();
        System.out.println("Name of the Student:"+str);
        if(ar==1 && cg>70 || ar==1|| ar==2 && cg>75){
            System.out.println(str+ " is Eligible for Placement");
        }
        else{
             System.out.println(str+ " is Not Eligible for Placement");
        }
    }
}
