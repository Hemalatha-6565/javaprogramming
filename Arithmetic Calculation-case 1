import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int b=scan.nextInt();
        char ch=scan.next().charAt(0);
        switch(ch){
            case '+':System.out.printf("Addition of two number is %.1f " ,(float)(a+b));
            break;
            case '-':System.out.printf("Subtraction of two number is %.1f" ,(float)(a-b));
            break;
            case '*':System.out.printf("Multiplication of two number is %.1f" , (float)(a*b));
            break;
            case '/':System.out.printf("Division  of two number is %.1f" , (float)(a/b));
            break;
            case '%':System.out.printf("Modulo of two number is %.1f" , (float)(a%b));
            break;
            default:System.out.println("Invalid Input");   
            break;
        }
    }
}
